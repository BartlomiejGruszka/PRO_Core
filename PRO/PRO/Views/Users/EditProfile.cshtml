@model PRO.UI.ViewModels.EditUserViewModel
@{
    ViewBag.Title = "Profil użytkownika";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div>
    <h1 class="main_h1 edit-info">
        <span class="h1-title">
            <span>
                @Model.AppUser.UserName
            </span>

        </span>
    </h1>
</div>
<div id="content" class="userprofilecontent">

    <partial name="~/Views/Users/_UserLeftSide.cshtml" , model="Model.AppUser" />

    <table border="0" cellpadding="0" cellspacing="0" width="100%">
        <tbody>
            <tr>

                <td valign="top" style="padding-left: 5px;">
                    <partial name="~/Views/Shared/_UserProfileEdit.cshtml" , model="Model" />

                </td>

            </tr>
        </tbody>
    </table>
</div>